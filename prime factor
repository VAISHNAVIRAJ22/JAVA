public class Main
{
    public static void fact(int n){
        if(n==0){
            return;
        }
        for(int i=2;i<=n;i++){
            if(n%i==0){
               System.out.print(i+" ");
               fact(n/i);
               return;
            }
        }
    }
	public static void main(String[] args) {
	fact(1080);
	}
}
